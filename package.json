    "@babel/plugin-transform-strict-mode": "^7.16.7",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@testing-library/react": "^16.0.0",
    "@wordpress/api-fetch": "^7.0.1",
    "@wordpress/block-editor": "^13.0.5",
